# Database Configuration
# Set DB_TYPE to 'sqlite' or 'mysql'
DB_TYPE=sqlite

# SQLite settings (used when DB_TYPE=sqlite)
SQLITE_PATH=/data/couple_cards.db

# MySQL settings (used when DB_TYPE=mysql)
MYSQL_HOST=mysql
MYSQL_PORT=3306
MYSQL_USER=couple_cards
MYSQL_PASSWORD=couple_cards_secret
MYSQL_DATABASE=couple_cards
